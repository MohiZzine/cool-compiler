class ktest {
    k: Int; 
    
    getK(): Int {
        k;
    };

    setK(new_k: Int): SELF_TYPE { 
        {
            k <- new_k; 
            self;
        }
    };
};

class Main inherits IO {
    main(): Object {
        let d1: ktest <- (new ktest).setK(2) in
            self.out_int(d1.getK()); -- Should print 2
    };
};

